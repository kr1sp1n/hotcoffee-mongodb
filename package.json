{
  "name": "hotcoffee-mongodb",
  "version": "0.0.1",
  "description": "Hotcoffee plugin to persist via MongoDB",
  "main": "index.coffee",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "make test"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kr1sp1n/hotcoffee-mongodb.git"
  },
  "keywords": [
    "hotcoffee",
    "mongodb",
    "plugin"
  ],
  "author": "kr1sp1n",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kr1sp1n/hotcoffee-mongodb/issues"
  },
  "homepage": "https://github.com/kr1sp1n/hotcoffee-mongodb",
  "dependencies": {
    "mongodb": "^1.4.10",
    "coffee-script": "^1.8.0"
  },
  "devDependencies": {
    "mocha": "2.0.1",
    "should": "^5.2.0",
    "sinon": "^1.14.0",
    "mocha-spec-cov-alt": "^0.2.4",
    "blanket": "^1.1.6",
    "mocha-lcov-reporter": "0.0.2",
    "coveralls": "^2.11.2"
  },
  "engines": {
    "node": "0.10.x",
    "npm": "1.2.x"
  },
  "config": {
    "blanket": {
      "pattern": [
        "index.coffee"
      ],
      "loader": "./node-loaders/coffee-script",
      "data-cover-never": [
        "node_modules",
        "test"
      ],
      "spec-cov": {
        "threshold": 100
      }
    }
  }
}
